project 'KSMoney-iOS.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'KSMoney-iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for KSMoney-iOS
  pod 'Alamofire', '~> 4.0'
  pod 'Kingfisher'
  pod 'SnapKit'
  pod 'SwiftyJSON'
  pod 'HandyJSON'
  pod 'SwiftyUserDefaults'
  pod 'Charts'
  pod 'CryptoSwift'
end
